<%- include('../partials/head', { title: 'Home - Login', styles: ['register'] }) %>
    
    <main>
        
        <div class="register-container">
            <img class="img-register" src="/img/img-register.jpg" alt="">
            <div class="container-form">
                <a href="/"><img class="logo-register" src="/img/logo-register.jpeg" alt="log"></a>
                <ul class="register-list">
                    <li class="nav-item"><button class="register-button"><a href="/user/register">Signup</a></button></li>
                    <li class="nav-item"><button class="register-button"><a href="#">Login</a></button></li>
                </ul>

                <form  action="/user/login" method="POST" class="register-form">
                
                    <label for="password">Email adress</label>
                    <input name="email" class="register-input" type="email" placeholder="Enter email">
                    <label for="password">Password</label>
                    <input name="password" class="register-input" type="password" placeholder="Enter password">
                    <button class="create-button">Login</button>

                </form>
            </div>

        </div>
    </main>
</body>
</html>